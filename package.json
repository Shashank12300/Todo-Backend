{
    "name": "todo-backend",
    "version": "1.0.0",
    "description": "Todo App",
    "main": "index.js",
    "scripts": {
        "start": "tsc-watch --project . --onSuccess \"npm run nodemon\"",
        "nodemon": "nodemon ./dist/index.js",
        "lint": "tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
        "lint:fix": "npm run lint --fix",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "@babel/core": "7.12.3",
        "@babel/node": "7.12.1",
        "@babel/preset-env": "7.12.1",
        "@types/express": "4.17.8",
        "commitizen": "4.2.2",
        "cz-conventional-changelog": "3.3.0",
        "husky": "4.3.0",
        "nodemon": "2.0.6",
        "tsc-watch": "4.2.9",
        "typescript": "4.0.3"
    },
    "dependencies": {
        "bcrypt": "5.0.0",
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "jsonwebtoken": "8.5.1",
        "mongoose": "5.10.11",
        "swagger-jsdoc": "5.0.1",
        "swagger-ui-express": "4.1.5",
        "tslint": "6.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run lint",
            "...": "..."
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Shashank12300/Todo-Backend.git"
    },
    "author": "Er. Shashank Baranwal",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Shashank12300/Todo-Backend/issues"
    },
    "homepage": "https://github.com/Shashank12300/Todo-Backend#readme"
}